<plugin name='database-session-flushable' version='1.2.5.8.6' grailsVersion='2.0.0 &gt; *'>
  <author>Robert Fischer</author>
  <authorEmail>robert.fischer@smokejumperit.com</authorEmail>
  <title>Database Session Flushable Plugin</title>
  <description>Stores HTTP sessions in a database</description>
  <documentation>http://grails.org/plugin/database-session</documentation>
  <type>DatabaseSessionFlushableGrailsPlugin</type>
  <resources>
    <resource>grails.plugin.databasesessionflushable.DatabaseCleanupJob</resource>
    <resource>grails.plugin.databasesessionflushable.DatabaseCleanupService</resource>
  </resources>
  <repositories>
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
    <repository name='grailsCentral' url='http://grails.org/plugins' />
    <repository name='http://repo.grails.org/grails/core' url='http://repo.grails.org/grails/core/' />
    <repository name='ebrRelease' url='http://repository.springsource.com/maven/bundles/release/' />
    <repository name='ebrExternal' url='http://repository.springsource.com/maven/bundles/external/' />
  </repositories>
  <dependencies>
    <compile>
      <dependency group='com.google.guava' name='guava' version='12.0' />
    </compile>
  </dependencies>
  <plugins />
  <runtimePluginRequirements>
    <plugin name='core' version='grailsVersion' />
  </runtimePluginRequirements>
  <behavior />
</plugin>